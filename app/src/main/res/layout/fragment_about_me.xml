        android:layout_width="325dp"
        android:layout_height="273dp"
        android:fontFamily="@font/geosans"
        android:text="Coquille \n \n Aplicación móvil para el mejoramiento en la atencion a niños con TDAH. \n Elaborado por: \n \nMyroslava Sánchez Andrade \n Uziel Humberto López Meneses \n Víctor Alfonso Mancera Ososrio \n Fernando de la Torre Rodriguez \n Pack de iconos: https://www.freepik.es/iconos-gratis/hada_14342601.htm#page=1query=fantasyposition=34from_view=search"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/vertical_guideline_10"
        app:layout_constraintEnd_toStartOf="@+id/vertical_guideline_8"
        app:layout_constraintStart_toStartOf="@+id/vertical_guideline_7"
        app:layout_constraintTop_toTopOf="@+id/vertical_guideline_13"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="64dp"
        android:layout_height="56dp"
