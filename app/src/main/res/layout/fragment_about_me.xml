        android:rotation="180"
        app:layout_constraintBottom_toTopOf="@+id/vertical_guideline_22"
        app:layout_constraintEnd_toStartOf="@+id/vertical_guideline_16"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/vertical_guideline_7"
        app:layout_constraintTop_toTopOf="@+id/vertical_guideline_21"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView152"
        android:layout_width="91dp"
        android:layout_height="101dp"
        android:src="@drawable/sf_logo"
        app:layout_constraintBottom_toTopOf="@+id/vertical_guideline_17"
        app:layout_constraintEnd_toStartOf="@+id/vertical_guideline_20"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/vertical_guideline_11"
        app:layout_constraintTop_toTopOf="@+id/vertical_guideline_14"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="95dp"
        android:layout_height="105dp"
        android:src="@drawable/tec_logo"
