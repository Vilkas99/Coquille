        app:layout_constraintEnd_toStartOf="@+id/vertical_guideline_12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/vertical_guideline_19"
        app:layout_constraintTop_toTopOf="@+id/vertical_guideline_14"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/geosans"
        android:text="Acerca de"
        android:textColor="@color/black"
        android:textSize="32sp"
        app:layout_constraintBottom_toTopOf="@+id/vertical_guideline_13"
        app:layout_constraintEnd_toStartOf="@+id/vertical_guideline_15"
        app:layout_constraintStart_toStartOf="@+id/vertical_guideline_7"
        app:layout_constraintTop_toTopOf="@+id/vertical_guideline_9"
